{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "order_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-detail.order-detail",
      "mappedBy": "product"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "product_coupon": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-coupon.product-coupon",
      "mappedBy": "product"
    },
    "currency": {
      "type": "string"
    },
    "stripe_id": {
      "type": "uid",
      "required": true
    },
    "price": {
      "type": "decimal"
    },
    "size": {
      "type": "string"
    },
    "description": {
      "type": "text"
    }
  }
}
